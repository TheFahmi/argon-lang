// Test Generic Call Syntax
// Tests: func<Type>(...)

struct Box<T> {
    value: T
}

fn wrap<T>(x: T) {
    return Box { value: x };
}

fn unwrap<T>(b: Box<T>) {
    return b.value;
}

fn main() {
    print("=== Generic Call Syntax ===");
    
    // Call with explicit type args
    let b1 = wrap<int>(42);
    print("wrap<int>(42) = " + b1.value);
    
    let b2 = wrap<string>("hello");
    print("wrap<string>(hello) = " + b2.value);
    
    // Nested
    let v = unwrap<int>(b1);
    print("unwrap<int>(b1) = " + v);
    
    print("=== Done ===");
    return 0;
}
